<Bindings>
    <Layer name="SI_KEYBINDINGS_LAYER_GENERAL">
	<Category name="CyroDoor">
	    <Action name="CYRODOOR_HOLDDOWN">
		<Down>
		    CyroDoor.Show(true)
		</Down>
                <Up>
                    CyroDoor.Show(false)
                </Up>
	    </Action>
	    <Action name="CYRODOOR_TOGGLE">
		<Down>
		    CyroDoor.Show()
		</Down>
	    </Action>
	</Category>
    </Layer>
</Bindings>
